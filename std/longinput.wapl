fn input_str():ptr:char{
    #=(a,malloc(1000005_,char),ptr:char);
    scanf("%1000000s",a);
    return a;
}
fn input_Strings(i64 n):ptr:String{
    #=(a,_,ptr:String);
    #=(sz,as(sizeof(String),isize),isize)
    =(a,malloc(*(sz,n),String));
    #=(i,0_,isize);
    loopif:(<(i,as(n,isize))){
        =([](a,i),String_from(input_str()));
        =(i,+(i,1));
    }
    return a;
}