use "./std/String.wapl"

fn input_i64(i64 n):ptr:i64{
    #=(a,_,ptr:i64);
    =(a,malloc(*(8_,n),i64));
    #=(i,0,i64);
    warpto(InputStart);
    point InputStart;
    warptoif(<(i,n),InputLoop,InputEnd);
    point InputLoop;
    scanf("%lld",ptr_add(a,i));
    =(i,+(i,1));
    warpto(InputStart)
    point InputEnd;
    warpto(Solve);
    point Solve;
    return a;
}
